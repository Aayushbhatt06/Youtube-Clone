    <div
      class="navbar flex justify-between items-center px-4 py-2 fixed top-0 bg-[#0f0f0f] z-10 w-full"
    >
      <div class="logo-ham flex items-center gap-6">
        <i
          class="fa-solid fa-bars fa-lg hamburger text-xl p-4 rounded-full hover:bg-[#272727]"
        ></i>
        <img class="logo invert w-[6vw] h-auto" src="svg/logo.svg" alt="logo" />
      </div>

      <div
        class="search-bar flex w-2/5 border border-[#333] rounded-full overflow-hidden bg-[#0f0f0f]"
      >
        <input
          class="flex-1 px-4 py-2 bg-transparent text-white placeholder-gray-400 text-base outline-none"
          type="text"
          placeholder="Search"
        />
        <button
          class="bg-[#1c1c1c] px-5 text-white flex cursor-pointer items-center justify-center"
        >
          <i class="fa fa-search"></i>
        </button>
      </div>

